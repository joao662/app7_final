<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.EDTProfessor">
    <ContentPage.Content>
        <StackLayout>

            <Picker x:Name="Picker" Title="Professores"></Picker>

            <Entry x:Name="Entry" Placeholder="Nome do Professor"/>

            <Entry x:Name="Entry2" Placeholder="CPF do Professor"/>

            <Entry x:Name="Entry3" Placeholder="Código do Professor"/>

            <Button x:Name="butSalvar" Text="Salvar Alterações"
                    HorizontalOptions="Center"
                    Clicked="OnButtonClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>